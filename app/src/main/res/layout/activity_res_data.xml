<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".res_data">




    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:src="@drawable/logo_l" />

    <RelativeLayout

        style="@style/main_page"
        android:layout_below="@+id/imageView"

        >


        <RelativeLayout
            android:id="@+id/navbar"
            style="@style/navbar"
            android:layout_width="match_parent">

            <ImageButton
                android:id="@+id/back_bt"
                style="@style/navbar_back_button"
                android:onClick="goBack"

                android:layout_above="@+id/nav_line"
                android:layout_toLeftOf="@+id/logout_tb"
                android:layout_toStartOf="@+id/logout_tb" />

            <ImageButton
                android:id="@+id/home_bt"
                style="@style/navbar_home_button"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:onClick="geHome" />

            <ImageButton
                android:id="@+id/logout_tb"
                style="@style/navbar_logout_button"

                android:layout_toLeftOf="@+id/home_bt"
                android:layout_toStartOf="@+id/home_bt"
                android:onClick="goOut"

                />

            <TextView
                android:id="@+id/mc_title"
                style="@style/navbar_title"
                android:text="@string/myres" />

            <ImageView
                android:id="@+id/nav_line"
                style="@style/line"


            android:layout_below="@+id/logout_tb"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

        </RelativeLayout>

        <RelativeLayout
            style="@style/main_data"

            android:layout_below="@+id/navbar">




            <ListView

                android:id="@+id/mc_list"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="0dp" />


        </RelativeLayout>
    </RelativeLayout>


</RelativeLayout>
