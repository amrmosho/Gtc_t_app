<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.pcland15.ismail.gtc.home">




    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:src="@drawable/logo_l" />

    <RelativeLayout

        style="@style/main_page"
        android:layout_below="@+id/imageView"

        >


        <RelativeLayout
            android:id="@+id/navbar"
            style="@style/navbar"
            android:layout_width="match_parent">

            <ImageButton
                android:id="@+id/home_bt"
                style="@style/navbar_back_button"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:onClick="goBack" />

            <ImageButton
                android:id="@+id/logout_bt"
                style="@style/navbar_logout_button"

                android:layout_toLeftOf="@+id/home_bt"
                android:layout_toStartOf="@+id/home_bt"
                android:onClick="goOut"

                />


            <TextView
                android:id="@+id/textView"
                style="@style/navbar_title"
                android:text="@string/aboutus" />

            <ImageView
                android:id="@+id/nav_line"
                style="@style/line"
                android:layout_below="@+id/textView" />
        </RelativeLayout>

        <RelativeLayout
            style="@style/main_data"

            android:layout_below="@+id/navbar"
            android:layout_alignParentBottom="true">


            <TextView
                android:textColor="@color/colorPrimary"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Amani Aziz Al-Amri   \n
                                                  201216017"
                android:id="@+id/textView2"
                android:textSize="15dp"


                android:layout_below="@+id/imageView2"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />


            <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
                android:textColor="@color/colorPrimary"
android:textSize="15dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Zahouah Hayel Al-Balwei
                 \n                                                  201216019"
                android:id="@+id/textView4"
                android:layout_below="@+id/textView2"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />


            <TextView
                android:textSize="17dp"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/colorPrimary"

                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Supervised by:\n
                          Dr - Marwan Al-Sayyed \n"
                android:id="@+id/textView3"
                android:layout_below="@+id/textView4"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="21dp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView2"
                android:src="@drawable/flogo"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true" />
        </RelativeLayout>
    </RelativeLayout>


</RelativeLayout>
